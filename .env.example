# 开发环境配置示例
NODE_ENV=development
PORT=3004

# 数据库配置
DATABASE_URL="file:./prisma/dev.db"

# CORS配置
CORS_ORIGIN=http://localhost:5173

# 文件上传配置
UPLOAD_DIR=./backend/uploads
MAX_FILE_SIZE=100MB

# 安全配置
SESSION_SECRET=your-session-secret-key
JWT_SECRET=your-jwt-secret-key

# 日志配置
LOG_LEVEL=debug

# 性能配置
MAX_CONNECTIONS=50
REQUEST_TIMEOUT=30000